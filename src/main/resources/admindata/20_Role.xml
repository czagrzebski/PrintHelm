<?xml version="1.0" encoding="UTF-8" ?>
<RoleData>
    <Role name="ROLE_ADMIN">
        <Description>Admin role</Description>
        <Privileges>
            <Privilege name="MANAGE_PRINTER"/>
            <Privilege name="MANAGE_USER"/>
            <Privilege name="MANAGE_APP_SETTING"/>
        </Privileges>
        <Inheritances>
            <Inherits role="ROLE_USER"/>
        </Inheritances>
    </Role>
    <Role name="ROLE_USER">
        <Description>User Role</Description>
        <Privileges>
            <Privilege name="START_PRINT"/>
            <Privilege name="PAUSE_PRINT"/>
            <Privilege name="STOP_PRINT"/>
            <Privilege name="VIEW_PRINTER"/>
            <Privilege name="CREATE_PRINT_JOB"/>
            <Privilege name="UPDATE_PRINT_JOB"/>
            <Privilege name="DELETE_PRINT_JOB"/>
        </Privileges>
    </Role>
</RoleData>